{
  "name": "claude-code-test",
  "version": "1.0.0",
  "description": "Playwright UIテストとデバッグ環境 - バイリンガル Hello World ページの包括的テストスイート",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "codegen": "playwright codegen http://localhost:3000",
    "trace": "playwright show-trace",
    "serve": "http-server . -p 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SakuraRinDev/claude-code-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SakuraRinDev/claude-code-test/issues"
  },
  "homepage": "https://github.com/SakuraRinDev/claude-code-test#readme",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "http-server": "^14.1.1"
  }
}
